  author = {McCulloch, Graham A. and Madeira, Paul T. and Makinson, Jeffrey R. and Dutoit, Ludovic and Blair, Zizah and Walter, Gimme H. and Nawaz, Muhammad and Purcell, Matthew F.},
  date = {2021},
  journaltitle = {Journal of Biogeography},
