@article{Porter2022,
  title = {{{MetaWorks}}: {{A}} Flexible, Scalable Bioinformatic Pipeline for High-Throughput Multi-Marker Biodiversity Assessments},
  author = {Porter, Teresita M. and Hajibabaei, Mehrdad},
  date = {2022},
  journaltitle = {PLoS ONE},
