@online{eenjesLinkedMachineLearning2022,
  title = {Linked Machine Learning Classifiers Improve Species Classification of Fungi When Using Error-Prone Long-Reads on Extended Metabarcodes},
  author = {Eenjes, Tavish and Hu, Yiheng and Irinyi, Laszlo and Hoang, Minh Thuy Vi and Smith, Leon M. and Linde, Celeste C. and Milgate, Andrew W. and Meyer, Wieland and Stone, Eric A. and Rathjen, John P. and Mashford, Benjamin and Schwessinger, Benjamin},
  date = {2022-04-06},
