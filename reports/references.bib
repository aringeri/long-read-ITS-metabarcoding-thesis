@article{kauserudITSAlchemyUse2023,
  title = {{{ITS}} Alchemy: {{On}} the Use of {{ITS}} as a {{DNA}} Marker in Fungal Ecology},
  shorttitle = {{{ITS}} Alchemy},
  author = {Kauserud, Håvard},
  date = {2023-10-01},
  journaltitle = {Fungal Ecology},
  shortjournal = {Fungal Ecology},
  volume = {65},
