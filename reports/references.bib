  author = {Hebert, Paul D.N. and Cywinska, Alina and Ball, Shelley L. and DeWaard, Jeremy R.},
  date = {2003},
  journaltitle = {Proceedings of the Royal Society B: Biological Sciences},
  volume = {270},
