@article{Begerow2010,
  title = {Current State and Perspectives of Fungal {{DNA}} Barcoding and Rapid Identification Procedures},
  author = {Begerow, Dominik and Nilsson, Henrik and Unterseher, Martin and Maier, Wolfgang},
  date = {2010},
  journaltitle = {Applied Microbiology and Biotechnology},
