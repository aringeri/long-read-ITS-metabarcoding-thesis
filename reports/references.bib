  author = {Větrovský, Tomáš and Kolaříková, Zuzana and Lepinay, Clémentine and Awokunle Hollá, Sandra and Davison, John and Fleyberková, Anna and Gromyko, Anastasiia and Jelínková, Barbora and Kolařík, Miroslav and Krüger, Manuela and Lejsková, Renata and Michalčíková, Lenka and Michalová, Tereza and Moora, Mari and Moravcová, Andrea and Moulíková, Štěpánka and Odriozola, Iñaki and Öpik, Maarja and Pappová, Monika and Piché-Choquette, Sarah and Skřivánek, Jakub and Vlk, Lukáš and Zobel, Martin and Baldrian, Petr and Kohout, Petr},
